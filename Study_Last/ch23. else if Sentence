# elif else - 예(1)
number = int(input("정수 입력: "))

if number > 0:
  print("양수입니다")
elif number < 0:
  print("음수입니다")
else :
  print("0입니다")


# elif else - 예(2)
month = int(input("월 입력: "))

if 3 <= month <= 5:
  print("봄입니다")
elif 6 <= month <= 8:
  print("여름입니다")
elif 9 <= month <= 11:
  print("가을입니다")
else:
  print("겨울입니다")
