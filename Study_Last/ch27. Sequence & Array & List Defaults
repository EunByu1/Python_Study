# 리스트 
list = [123, "abc", True]

print(type(list[0]))
print(type(list[1]))
print(type(list[2]))
print()

print(list[0:3])
print()

print(list[1][1])
print()

print(list + list)
print(list * 5)
print(type(list + list))
print(type(list * 5))
print()

print(len(list))
print(type(len(list)))


# 리스트 반대로 돌리기
a = [1, 2, 3, 4, 5]
print(a[::-1])


# 중첩 리스트 
b = [a, a, a]
print(b)
